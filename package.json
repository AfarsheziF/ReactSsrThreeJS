  "scripts": {
    "start": "cross-env NODE_ENV=production node public/ReactSsrThreeJsTemp.server.bundle.js",
    "webpack:client": "webpack --config webpack.client.js --watch",
    "webpack:server": "webpack --config webpack.server.js --watch",
    "webpack:start": "nodemon --watch public --exec node --inspect public/ReactSsrThreeJsTemp.server.bundle.js",
    "dev": "cross-env sendErrorEmails=\"true\" NODE_ENV=development npm-run-all --parallel webpack:*",
    "dev_linux": "cross-env NODE_OPTIONS=--openssl-legacy-provider sendErrorEmails=\"true\" NODE_ENV=development npm-run-all --parallel webpack:*",
    "build": "cross-env NODE_ENV=production webpack --mode production --config webpack.client.js && webpack --mode production --config webpack.server.js",
    "build_dev": "cross-env sendErrorEmails=\"true\" NODE_ENV=development webpack --config webpack.client.js && webpack --config webpack.server.js",
    "build_linux": "cross-env NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=production npm-run-all linux:*",
    "build_render": "cross-env NODE_ENV=production npm-run-all linux:*",
    "linux:client": "webpack --mode production --config webpack.client.js",
    "linux:server": "webpack --mode production --config webpack.server.js"
  },
  "engines": {
    "node": ">=20"
  }
